{
  "name": "@serenity-js/protractor",
  "version": "3.0.0-rc.11",
  "description": "Test your web apps with Serenity/JS",
  "author": {
    "name": "Jan Molak",
    "email": "jan.molak@smartcodeltd.co.uk",
    "url": "https://janmolak.com"
  },
  "funding": {
    "url": "https://github.com/sponsors/serenity-js"
  },
  "homepage": "https://serenity-js.org",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "keywords": [
    "serenity-js",
    "ui",
    "frontend",
    "angular",
    "react",
    "vue",
    "bdd",
    "tdd",
    "test",
    "testing"
  ],
  "scripts": {
    "clean": "rimraf .nyc_output lib target",
    "lint": "eslint --ext ts --config ../../.eslintrc.yml .",
    "lint:fix": "npm run lint -- --fix",
    "debug": "node --inspect-brk node_modules/.bin/protractor ./spec/protractor.conf.js",
    "test": "nyc --report-dir ../../target/coverage/protractor protractor ./spec/protractor.conf.js",
    "compile": "tsc --project tsconfig.json",
    "site": "esdoc -c .esdoc.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serenity-js/serenity-js.git"
  },
  "bugs": {
    "url": "https://github.com/serenity-js/serenity-js/issues"
  },
  "engines": {
    "node": "^14 || ^16",
    "npm": "^6 || ^7 || ^8"
  },
  "dependencies": {
    "@serenity-js/assertions": "3.0.0-rc.11",
    "@serenity-js/core": "3.0.0-rc.11",
    "deepmerge": "^4.2.2",
    "is-plain-object": "^5.0.0",
    "query-selector-shadow-dom": "^1.0.0",
    "tiny-types": "^1.17.0"
  },
  "peerDependencies": {
    "@serenity-js/cucumber": "^3.0.0-rc",
    "@serenity-js/jasmine": "^3.0.0-rc",
    "@serenity-js/mocha": "^3.0.0-rc",
    "protractor": "^5.0.0 || ^7.0.0",
    "selenium-webdriver": "^3.6.0"
  },
  "peerDependenciesMeta": {
    "@serenity-js/cucumber": {
      "optional": true
    },
    "@serenity-js/jasmine": {
      "optional": true
    },
    "@serenity-js/mocha": {
      "optional": true
    }
  },
  "devDependencies": {
    "@documentation/esdoc-template": "3.0.0",
    "@integration/testing-tools": "3.0.0",
    "@serenity-js/cucumber": "3.0.0-rc.11",
    "@serenity-js/jasmine": "3.0.0-rc.11",
    "@serenity-js/local-server": "3.0.0-rc.11",
    "@serenity-js/mocha": "3.0.0-rc.11",
    "@serenity-js/rest": "3.0.0-rc.11",
    "@serenity-js/web": "3.0.0-rc.11",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/html-minifier": "^4.0.2",
    "@types/mocha": "^9.0.0",
    "@types/selenium-webdriver": "^3.0.19",
    "axios": "^0.21.4",
    "body-parser": "^1.19.1",
    "cross-env": "^7.0.3",
    "express": "^4.17.2",
    "html-minifier": "^4.0.0",
    "mocha": "^9.1.3",
    "protractor": "^7.0.0",
    "selenium-webdriver": "^3.6.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "lib",
      "node_modules",
      "spec"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json"
    ],
    "cache": true,
    "all": true
